<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"] || Cửa hàng âm nhạc</title>
    <!-- Stylesheets -->
    <link href="~/css/bootstrap.css" rel="stylesheet">
    <link href="~/css/revolution-slider.css" rel="stylesheet">
    <link href="~/css/style.css" rel="stylesheet">
    <!-- Responsive -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link href="~/css/responsive.css" rel="stylesheet">
</head>
<body>
    <div class="page-wrapper">
        <header class="main-header fixed-header" style="box-shadow: 0 3px 10px rgb(0 0 0 / 0.3);">
            <div class="container-Header clearfix">
                <div class="logo inline-element">
                    <img class="logo-header" src="~/image/logobrand/main-logo.png" alt="An Hoa">
                </div>
                <div class="nav-outer-hd clearfix">
                    <nav class="main-menu inline-element">
                        <div class="navbar-header" style="margin:27px 0;">
                            <button type="button" class="navbar-toggle" id="menuToggle">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                        <nav id="sideNav" class="side-nav">
                            <div class="menu-header"><span class="head-title-menu">Menu</span><button class="close-btn" id="closeBtn">×</button></div>
                            <div class="item-menu">
                                <ul>
                                    <li><a href="/">Trang Chủ</a></li>
                                    <li><a href="/Store">Cửa hàng</a></li>
                                    <li>@await Html.PartialAsync("_CartPartial")</li>
                                    <li>
                                        <partial name="_loginpartial" />
                                    </li>
                                    <partial name="_AdminMenu" />
                                </ul>
                            </div>
                        </nav>
                        <div id="overlay" class="overlay"></div>
                        <div class="navbar-collapse collapse clearfix">
                            <ul class="navigation">
                                <li><a href="/">Trang Chủ</a></li>
                                <li> <a href="/Store/" id="current">Cửa hàng</a></li>
                                <li>@await Html.PartialAsync("_CartPartial")</li>
                               
                                <partial name="_loginpartial" />
                                <partial name="_AdminMenu" />
                               
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>
        </header>

        @RenderBody()

        <footer class="main-footer">

            <!--Footer Upper-->
            <div class="footer-upper">
                <div class="auto-container">
                    <div class="row clearfix">
                        <div class="logo text-center">
                        </div>
                        @*    <!--Two 4th column-->
                        <div class="col-md-7 col-sm-12 col-xs-12">
                        <div class="row clearfix">
                        <div class="col-lg-7 col-sm-6 col-xs-12 column">
                        <h2>Music Hit</h2>
                        <div class="footer-widget about-widget">
                        <div class="textcontentft">
                        <span>Sức khỏe/Sắc đẹp. Mua sắm & bán lẻ</span>
                        </div>
                        <ul class="contact-info">
                        <li><span class="icon fa fa-map-marker"></span>Số 5, ngõ 8 Phố Hoa Lư, Lê Đại Hành, Hai Bà Trưng, Hà Nội</li>
                        <li><span class="icon fa fa-phone"></span>0989 633 688</li>
                        <li><i class="icon fa fa-globe"></i>www.Anhoavn.com</li>
                        </ul>
                        <div class="social-links-two clearfix">
                        <a href="#" class="facebook img-circle"><span class="fa fa-facebook-f"></span></a>
                        <a href="#" class="twitter img-circle"><span class="fa fa-twitter"></span></a>
                        <a href="#" class="linkedin img-circle"><span class="fa fa-linkedin"></span></a>
                        </div>
                        </div>
                        </div>
                        <!--Footer Column-->
                        <div class="col-lg-5 col-sm-6 col-xs-12 column">
                        <h2>SẢN PHẨM AN HOA</h2>
                        <div class="footer-widget links-widget">
                        <ul>
                        <li><a href="#">Thực phẩm chức năng</a></li>
                        <li><a href="#">Mỹ phẩm</a></li>
                        <li><a href="#">Nước hoa</a></li>
                        <li><a href="#">Giảm cân</a></li>
                        <li><a href="#">Hỗ trợ tập luyện</a></li>
                        </ul>
                        </div>
                        </div>
                        </div>
                        </div>
                        <!--Two 4th column End-->
                        <!--Two 4th column-->
                        <div class="col-md-5 col-sm-12 col-xs-12">
                        <div class="row clearfix">
                        <div class="col-lg-6 col-sm-6 col-xs-12 column">
                        <h2>TIN TỨC</h2>
                        <div class="footer-widget links-widget">
                        <ul>
                        <li><a href="#">Thực phẩm chức năng</a></li>
                        <li><a href="#">Mỹ phẩm</a></li>
                        <li><a href="#">Nước hoa</a></li>
                        <li><a href="#">Giảm cân</a></li>
                        <li><a href="#">Hỗ trợ tập luyện</a></li>
                        </ul>
                        </div>
                        </div>
                        <!--Footer Column-->
                        <div class="col-lg-6 col-sm-6 col-xs-12 column">
                        <div class="footer-widget links-widget">
                        <h2>CƠ HỘI KINH DOANH</h2>
                        <ul>
                        <li><span>Bạn muốn làm</span></li>
                        <li><span class="text-white">Đại lý phân phối</span></li>
                        <li><span class="text-white">Nhượng quyền An Hoa</span></li>
                        </ul>
                        </div>
                        </div>
                        </div>
                        </div> *@
                    </div>
                </div>
            </div>
            <div class="footer-bottom">
                <div class="auto-container clearfix">
                    <div class="copyright text-center">Music store &copy; 2024. All rights reserved </div>
                </div>
            </div>
        </footer>
    </div>
    <script src="~/js/jquery.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/revolution.min.js"></script>
    <script src="~/js/jquery.fancybox.pack.js"></script>
    <script src="~/js/jquery.fancybox-media.js"></script>
    <script src="~/js/owl.js"></script>
    <script src="~/js/wow.js"></script>
    <script src="~/js/script.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
